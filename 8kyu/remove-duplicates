// Define a function that removes duplicates from an array of numbers and returns it as a result.

// The order of the sequence has to stay the same.

// My Solution
function distinct(a) {
let arr = []
for (let i = 0; i<a.length; i++){
  if (!arr.includes(a[i])){
    arr.push(a[i])
  }
}return arr
}

//Another
function distinct(a) {
    return [...new Set(a)];
  }