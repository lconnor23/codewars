// Given a non-empty array of integers, return the result of multiplying the values together in order. Example:

// [1, 2, 3, 4] => 1 * 2 * 3 * 4 = 24

//My Solution
function grow(x){
  // array numbers
  //return product multiplying all together
  n = 1
  for (i = 0; i < x.length; i++){
    n = n * x[i]
  }
 return n
}

//Other Solutions
const grow=x=> x.reduce((a,b) => a*b);