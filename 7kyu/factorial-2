// Your task is to write function factorial.

// https://en.wikipedia.org/wiki/Factorial

// My solution
function factorial(n){
  let product = 1
  for(let i = 1; i<=n; i++){
    product *= i
  }return product
}

//Another
function factorial(n){
    let answer = 1;
    
    while (n > 0) {
      answer *= n;
      n--;
    }
    
    return answer;
  }

  //Recursive solution
  const factorial = n => n ? factorial(n - 1) * n : 1;